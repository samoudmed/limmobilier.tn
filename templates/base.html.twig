        <script>
            document.addEventListener('DOMContentLoaded', function () {
                const menuToggle = document.querySelector('.mobile-menu-toggle');
                const mainMenu = document.querySelector('#pbr-mainmenu');
                const userActions = document.querySelector('.user-actions');

                if (menuToggle && mainMenu) {
                    // We will manage the overflow property here in the JS
                    // Make sure the menu starts as hidden
                    mainMenu.style.overflow = 'hidden';

                    menuToggle.addEventListener('click', function () {
                        // Check if the menu is currently open or opening
                        if (mainMenu.classList.contains('is-open')) {
                            // IT'S OPEN, SO CLOSE IT
                            // 1. Immediately hide overflow to prevent content from spilling out during animation
                            // 2. Remove the class to trigger the CSS max-height transition
                            mainMenu.classList.remove('is-open');
                            userActions.classList.remove('is-open');
                        } else {
                            // IT'S CLOSED, SO OPEN IT
                            // 1. Add the class to trigger the CSS max-height transition
                            mainMenu.classList.add('is-open');
                            userActions.classList.add('is-open');
                            // 2. Wait for the CSS animation to finish, THEN make overflow visible
                        }

                        // Update aria-expanded for accessibility
                        const isOpen = mainMenu.classList.contains('is-open');
                        this.setAttribute('aria-expanded', isOpen);
                    });
                }
            });
        </script>
