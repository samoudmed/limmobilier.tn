{% extends 'base.html.twig' %}
{% block meta %}
    <title>Tunisie annonce immobilier Vente Location Immobilier Tunisie</title>
    <meta name="description" content="Tunisie annonce immobilier découvrez les meilleures offres de vente et location immobilier Tunisie appartement, une maison, un terrain ou un fond de commerce" />
{% endblock %}
{% block open_graph %}<link rel="canonical" href="https://limmobilier.tn" />
    <meta property="og:url" content="https://limmobilier.tn" />
    <meta property="og:type" content="article" />
    <meta property="og:title" content="Trouvez un appartement ou une maison en vente ou en location en Tunisie" />
    <meta property="og:description" content="Annonces immobilières en Tunisie Trouvez un appartement, une maison, un terrain ou un fond de commerce en vente ou en location" />
    <meta property="og:image" content="{{ asset('uploads/photos/affiche.png') }}" />
{% endblock %}
{% block jquery %}{% endblock %}
{% block body %}
    <section class="main-search main-search--absolute">
        <div id="content-list" class="container">
            <br>
            <div class="main-search__container">
                <section class="listing-search">
                    <form method="get" action="{{ path('annonce_search') }}">
                        <input type="text" name="q" placeholder="Rechercher..." />
                        <button type="submit">Rechercher</button>
                    </form>
                    <form action="" method="get" class="listing-search__form">
                        <div class="row">
                            <div class="col-sm-5">
                                <label for="offer-type" class="listing-search__label">{{'Type'|trans}}</label>
                                <select name="offer-type" id="offer-type" class="main-listing__form-field js-example-basic-single" style="width: 100%;">
                                    <option value="All" selected="">{{'Tous'|trans}}</option>
                                    <option value="Location">{{'Location'|trans}}</option>
                                    <option value="Vente">{{'Vente'|trans}}</option>
                                </select>
                            </div><!-- .col -->
                            <div class="col-sm-5">
                                <label for="keywords" class="listing-search__label">{{'Type de bien'|trans}}</label>
                                <select name="bien-type" id="bien-type" class="main-listing__form-field js-example-basic-single" style="width: 100%;">
                                    <option value="All" selected="">{{'Tous'|trans}}</option>
                                    <option value="1">{{'Appartement'|trans}}</option>
                                    <option value="2">{{'Villa'|trans}}</option>
                                    <option value="3">{{'Terrain'|trans}}</option>
                                    <option value="4">{{'Projet neuf'|trans}}</option>
                                    <option value="5">{{'Bureau'|trans}}</option>
                                    <option value="6">{{'Commerce'|trans}}</option>
                                    <option value="7">{{'Industrie'|trans}}</option>
                                    <option value="8">{{'Dépôt'|trans}}</option>
                                    <option value="9">{{'Vacances'|trans}}</option>
                                    <option value="10">{{'Autre'|trans}}</option>
                                </select>
                            </div><!-- .col -->
                            <div class="col-sm-2">
                                <label for="listing-btn" class="listing-search__label listing-search__label--hidden">{{'Recherche avancée'|trans}}</label>
                                <a href="#" id="listing-btn" class="listing-search__btn">{{'Recherche avancée'|trans}}</a>
                            </div><!-- .col -->
                        </div><!-- row -->
                    </form><!-- .listing-search__form -->
                </section><!-- .listing-search -->
            </div><!-- .main-search__container -->
        </div><!-- .container -->
    </section>
    <section class="item-grid">
